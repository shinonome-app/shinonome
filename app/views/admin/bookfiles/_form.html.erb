<%= form_with(model: [:admin, bookfile]) do |form| %>
  <% if bookfile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(bookfile.errors.count, "error") %> prohibited this bookfile from being saved:</h2>

