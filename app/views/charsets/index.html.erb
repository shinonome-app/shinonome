<p id="notice"><%= notice %></p>

<h1>Charsets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @charsets.each do |charset| %>
      <tr>
        <td><%= charset.name %></td>
        <td><%= link_to 'Show', charset %></td>
        <td><%= link_to 'Edit', edit_charset_path(charset) %></td>
        <td><%= link_to 'Destroy', charset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Charset', new_charset_path %>
