<p id="notice"><%= notice %></p>

<h1>File Encodings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_encodings.each do |file_encoding| %>
      <tr>
        <td><%= file_encoding.name %></td>
        <td><%= link_to 'Show', file_encoding %></td>
        <td><%= link_to 'Edit', edit_file_encoding_path(file_encoding) %></td>
        <td><%= link_to 'Destroy', file_encoding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File Encoding', new_file_encoding_path %>
