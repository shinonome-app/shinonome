<p id="notice">
  <%= notice %>
</p>
<h1>Proofreads</h1>
<table>
  <thead>
    <tr>
      <th>Work</th>
      <th>Work copy</th>
      <th>Work print</th>
      <th>proof_edition</th>
      <th>Workfile</th>
      <th>Address</th>
      <th>Memo</th>
      <th>Worker</th>
      <th>worker kana</th>
      <th>Worker name</th>
      <th>Email</th>
      <th>Url</th>
      <th>Person</th>
      <th>assign_status</th>
      <th>order_status</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @proofreads.each do |proofread| %>
      <tr>
        <td>
          <%= proofread.work_id %>
        </td>
        <td>
          <%= proofread.work_copy %>
        </td>
        <td>
          <%= proofread.work_print %>
        </td>
        <td>
          <%= proofread.proof_edition %>
        </td>
        <td>
          <%= proofread.workfile_id %>
        </td>
        <td>
          <%= proofread.address %>
        </td>
        <td>
          <%= proofread.memo %>
        </td>
        <td>
          <%= proofread.worker_id %>
        </td>
        <td>
          <%= proofread.worker_kana %>
        </td>
        <td>
          <%= proofread.worker_name %>
        </td>
        <td>
          <%= proofread.email %>
        </td>
        <td>
          <%= proofread.url %>
        </td>
        <td>
          <%= proofread.person_id %>
        </td>
        <td>
          <%= proofread.assign_status %>
        </td>
        <td>
          <%= proofread.order_status %>
        </td>
        <td>
          <%= link_to 'Show', proofread %>
        </td>
        <td>
          <%= link_to 'Edit', edit_proofread_path(proofread) %>
        </td>
        <td>
          <%= link_to 'Destroy', proofread, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Proofread', new_proofread_path %>
